{
  "name": "trois-renderer-proof-of-concept",
  "version": "0.1.1368",
  "scripts": {
    "dev": "vite -c utils/vite.config.ts",
    "build": "vue-tsc --noEmit && vite build -c utils/vite.config.ts",
    "build:lib": "tsc --project ./utils/tsconfig.lib.json && npm run build:rollup && npm run dts:create",
    "build:rollup": "rollup -c ./utils/lib-rollup.ts",
    "dts:create": "cp ./utils/lib-dts.d.ts ./dist/trois-renderer-proof-of-concept.es.d.ts && cp ./utils/lib-dts.d.ts ./dist/trois-renderer-proof-of-concept.umd.d.ts",
    "prepare": "npm run build:lib"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.0.4",
    "@types/chroma-js": "2.1.3",
    "@types/lodash": "4.14.170",
    "@types/luxon": "1.27.1",
    "@types/three": "0.129.2",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "cannon-es": "0.18.0",
    "chroma-js": "2.1.2",
    "luxon": "2.0.1",
    "popmotion": "8.7.6",
    "postprocessing": "6.22.2",
    "rollup": "2.56.3",
    "rollup-plugin-terser": "7.0.2",
    "simplex-noise": "2.4.0",
    "three": "0.130.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.8",
    "vite-dts": "1.0.3",
    "vue": "^3.0.5",
    "vue-tsc": "^0.0.24"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "src"
  ],
  "main": "./dist/trois-renderer.js",
  "module": "./dist/trois-renderer.module.js",
  "types": "dist/trois-renderer-proof-of-concept.es.d.ts"
}